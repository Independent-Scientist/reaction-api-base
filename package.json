    "eslint": "3.19.0",
    "eslint-plugin-babel": "4.1.1",
    "mongodb-prebuilt": "6.3.3",
    "nodemon": "1.11.0",
    "rimraf": "2.6.1"
  },
  "dependencies": {
    "babel-runtime": "6.23.0",
    "bcrypt": "1.0.2",
